    "keywords":["bootstrap", "layout", "contao"],
    "type":"contao-module",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"David Molineus",
            "email":"mail@netzmacht.de",
            "homepage":"http://www.netzmacht.de",
            "role":"Project leader"
        }
    ],
    "support":{
        "email":"mail@netzmacht.de",
        "issues":"https://github.com/contao-bootstrap/layout/issues",
        "wiki":"http://contao-bootstrap.netzmacht.de",
        "source":"https://github.com/contao-bootstrap/layout"
    },
    "require":{
        "php":">=5.3.4",
        "contao-community-alliance/composer-installer": "~1.6",
        "contao/core":">=3.1,<3.3",
        "netzmacht/html": "*",
        "netzmacht/contao-bootstrap-core": "*"
